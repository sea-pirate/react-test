{
  "name": "react-testing",
  "version": "1.0.0",
  "description": "Minimalist app setup with custom config for React, Babel, and Webpack without create-react-app",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "webpack --mode production --config webpack.config.prod.js",
    "clean": "rm dist/* -r",
    "dev": "npm-run-all --parallel watch:server watch:build",
    "watch:server": "nodemon index.js",
    "watch:build": "webpack --watch"
  },
  "keywords": [],
  "author": "\"Joe Momma\"",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
}
